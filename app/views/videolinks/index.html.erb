<p id="notice"><%= notice %></p>

<h1>Listing Videolinks</h1>

<table>
  <thead>
    <tr>
      <th>Link</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videolinks.each do |videolink| %>
      <tr>
        <td><%= videolink.link %></td>
        <td><%= videolink.User %></td>
        <td><%= link_to 'Show', videolink %></td>
        <td><%= link_to 'Edit', edit_videolink_path(videolink) %></td>
        <td><%= link_to 'Destroy', videolink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Videolink', new_videolink_path %>
